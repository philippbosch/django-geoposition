#: apps.py:7
msgid "Geoposition"
msgstr "Geoposición"

#: fields.py:12
msgid "A geoposition field to store latitude, longitude and elevation data"
msgstr ""
"Campo de geoposición para almecenar datos de latitud, longitud y elevación"

#: forms.py:11
msgid "Enter a valid value for latitude."
msgstr "Ingrese un valor adecuado para la latitud."

#: forms.py:12
msgid "Enter a valid value for longitude."
msgstr "Ingrese un valor adecuado para la longitud."

#: forms.py:13
msgid "Enter a valid value for elevation."
msgstr "Ingrese un valor adecuado para la elevación."

#: forms.py:40 widgets.py:48
msgid "latitude"
msgstr "latitud"

#: forms.py:48
msgid "latitude must be a value between -90 and +90 degrees"
msgstr "la latitud debe estar comprendida entre 90 y -90 grados"

#: forms.py:53 widgets.py:53
msgid "longitude"
msgstr "longitud"

#: forms.py:61
msgid "longitude must be a value between -180 and +180 degrees"
msgstr "la longitud debe estar comprendida entre -180 y 180 grados"

#: forms.py:66 widgets.py:58
msgid "elevation"
msgstr "elevación"

#: validators.py:21
#, python-format
msgid "This value must be between %(lower)s and %(upper)s."
msgstr "Este valor debe estar comprendido entre %(lower)s y %(upper)s."

#: validators.py:23
#, python-format
msgid "This value must be at least %(lower)s."
msgstr "Este no debe ser menor que %(lower)s."

#: validators.py:25
#, python-format
msgid "This value must be no more than %(upper)s."
msgstr "Este valor no debe ser mayor que %(upper)s."
